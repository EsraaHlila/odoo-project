<odoo>
    <data noupdate="1">
        <record id="ir_cron_sync_runner" model="ir.cron">
            <field name="name">Run Sync Scripts</field>
            <field name="model_id" ref="model_sync_runner"/>
            <field name="state">code</field>
            <field name="code">model.run_all_scripts()</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
			<field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
